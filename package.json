		"@sinclair/typebox": "^0.22.1",
		"@typegoose/typegoose": "^9.2.0",
		"@types/node": "^18.0.6",
		"crypto-random-string": "^4.0.0",
		"debug": "~4.3.3",
		"discord-oauth2": "^2.8.0",
		"discord.js": "^13.3.1",
		"dotenv": "^10.0.0",
		"envalid": "^7.2.2",
		"eslint": "^8.3.0",
		"fagc-api-types": "^1.3.0",
		"fastify": "^3.24.0",
