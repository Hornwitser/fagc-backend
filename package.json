		"@types/node-fetch": "^3.0.3",
		"@types/nodemon": "^1.19.1",
		"@types/prettier": "^2.4.2",
		"@types/ws": "^8.2.2",
		"@typescript-eslint/eslint-plugin": "^5.6.0",
		"@typescript-eslint/parser": "^4.31.0",
